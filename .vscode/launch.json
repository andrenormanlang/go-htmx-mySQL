{
    // Use IntelliSense to learn about possible attributes.
   // Hover to view descriptions of existing attributes.
   // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
   "version": "0.2.0",
   "configurations": [
       {
           "name": "CMSGOApp",
           "type": "go",
           "request": "launch",
           "mode": "auto",
           "program": "${workspaceFolder}/cmd/cmsgo",
           "cwd": "${workspaceFolder}",
           "envFile": "${workspaceFolder}/envfile",
           "args": [
               "--config",
               "${workspaceFolder}/cmsgo_config.toml"
           ],
           "showLog": true,
           "trace": "log"
       },
       {
           "name": "AdminApp",
           "type": "go",
           "request": "launch",
           "mode": "auto",
           "program": "${workspaceFolder}/cmd/cmsgo-admin",
           "cwd": "${workspaceFolder}",
           "envFile": "${workspaceFolder}/envfile",
           "args": [
               "--config",
               "${workspaceFolder}/cmsgo_config.toml"
           ],
           "showLog": true,
           "trace": "log"
       },
   ]
}